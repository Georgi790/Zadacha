<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/song_vote"
    app:startDestination="@id/enterFragment">

    <fragment
        android:id="@+id/enterFragment"
        android:name="com.example.zadacha.enterFragment"
        android:label="fragment_enter"
        tools:layout="@layout/fragment_enter" >
        <action
            android:id="@+id/action_enterFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_enterFragment_to_voteFragment"
            app:destination="@id/voteFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.zadacha.registerFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/voteFragment"
        android:name="com.example.zadacha.voteFragment"
        android:label="fragment_vote"
        tools:layout="@layout/fragment_vote" />
</navigation>